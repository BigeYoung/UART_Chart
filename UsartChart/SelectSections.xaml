<UserControl x:Class="UsartChart.SelectSections"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UsartChart"
             mc:Ignorable="d" Background="White"
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel Margin="10">
        <Button MinHeight="28" Margin="0,10" DockPanel.Dock="Top" Click="SelectELF_Click">读取elf文件</Button>
        <DataGrid x:Name="m_DataGrid" ItemsSource="{Binding sections}" ColumnWidth="*"></DataGrid>
    </DockPanel>
</UserControl>
