<Window x:Class="UsartChart.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UsartChart"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="960">
    <Window.DataContext>
        <local:SpeedTestVm></local:SpeedTestVm>
    </Window.DataContext>
    <DockPanel>
        <local:StatusBar x:Name="m_StatusBar" DockPanel.Dock="Bottom" Height="28"></local:StatusBar>

        <StackPanel DockPanel.Dock="Left">
            <local:UART></local:UART>
            <local:SelectSections Width="240"></local:SelectSections>
        </StackPanel>
        <local:Chart></local:Chart>
        
    </DockPanel>
</Window>
